{
    "name": "carte-blanche/bundle-gitviewer",
    "type": "carte-blanche-bundle",
    "description": "A bundle to build a GIT repository browsing",
    "keywords": [ "carte blanche", "bundles" ],
    "homepage": "http://github.com/php-carteblanche/bundle-gitviewer",
    "license": "Apache-2.0",
    "authors": [{
        "name": "Les Ateliers Pierrot & contributors",
        "email": "contact@ateliers-pierrot.fr",
        "homepage": "http://www.ateliers-pierrot.fr/"
    }],
    "require": {
        "atelierspierrot/git-api": "1.*",
        "carte-blanche/core": "dev-master",
        "carte-blanche/installer": "dev-master"
    },
    "autoload": { 
        "psr-0": { "GitViewer": "src" },
        "classmap": [ "src/GitViewer/Controller" ]
    },
    "repositories": [{
        "type": "composer", "url": "http://packagist.ateliers-pierrot.fr/"
    }],
    "extra": {
        "views": "src/GitViewer/views",
        "carte-blanche-configs": "config/gitviewer_config.ini",
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
